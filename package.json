    "webpack-git-hash": "^1.0.2"
  },
  "scripts": {
    "clean-dist": "del-cli dist/*",
    "copy-resources": "make-dir dist && cpy 'resources/**/*.*' dist",
    "bundle-app": "webpack --config webpack/webpack-application-config.js --bail",
    "bundle-vendor": "webpack --config webpack/webpack-vendor-config.js",
    "build": "npm run clean-dist && npm run copy-resources && npm run bundle-vendor && npm run bundle-app",
    "build-dev": "cross-env-shell NODE_ENV=development \"npm run clean-dist && npm run copy-resources && npm run bundle-vendor && npm run bundle-app\"",
    "build-local": "cross-env-shell NODE_ENV=local-development \"del-cli dist/index.html && npm run copy-resources && npm run bundle-vendor\"",
    "staging": "cross-env-shell NODE_ENV=development \"npm run build-dev && node server/express-server.js\"",
    "production": "cross-env-shell NODE_ENV=production \"npm run build && node server/express-server.js\"",
    "local": "cross-env-shell NODE_ENV=local-development \"npm run build-local && node server/express-server.js\"",
    "lint": "concurrently \"eslint . --ext .ts,.tsx\" \"tsc --pretty --noEmit\"",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "Chrome > 63",
    "Firefox > 57",
    "Edge > 15",
    "Safari > 10",
    "iOS > 10"
  ],
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/superagent": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.7.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^23.0.1",
    "mobx-react-devtools": "^6.1.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "16.8.3",
    "sass-lint": "^1.13.1",
    "ts-jest": "^23.0.1",
    "tsconfig-paths-jest": "^0.0.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
